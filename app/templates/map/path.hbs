{{path-editor
  viewport=mapViewport
  layers=layers
  lines=model.lines
  add=(action 'addLine')
  select=(action 'selectLine')
  deselectAll=(action 'deselectAll')
  resize=(action 'resizeLine')
  remove=(action 'removeLines')}}

<aside class="ui-modules">
  {{map-mode-navigator}}
  {{path-item}}
  {{path-timeline events=model.events}}
</aside>
