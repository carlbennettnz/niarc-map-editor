{{path-editor
  viewport=mapViewport
  layers=layers
  map=model.map
  events=model.events
  add=(action 'addPath')
  select=(action 'selectShape')
  deselectAll=(action 'deselectAll')
  resize=(action 'resizeShape')
  remove=(action 'removeShapes')}}

<aside class="ui-modules">
  {{map-mode-navigator}}
  {{path-item}}
  {{path-timeline events=model.events}}
</aside>
